<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PraiseListView">
        <attr name="item_color" format="color"/>
        <attr name="item_selector_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="ExpandTextView">
        <attr name="showLines" format="integer"/>
    </declare-styleable>

    <declare-styleable name="NineGridImageView">
        <attr name="imgGap" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="ProgressButton">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension" />
        <attr name="max" format="integer" />
    </declare-styleable>

    <declare-styleable name="CharIndexView">
        <!-- 	banner背影色  	 -->
        <!-- 索引字大小 -->
        <attr name="indexTextSize" format="dimension"/>
        <!-- 字颜色 -->
        <attr name="charTextColor" format="color"/>
        <!-- 索引字颜色 -->
        <attr name="indexTextColor" format="color"/>
    </declare-styleable>

</resources>